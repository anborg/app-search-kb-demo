{
  "extends": "strongloop",
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
   "max-len": ["error", 120, 2, {
    "ignoreUrls": true,
    "ignoreComments": false,
    "ignoreRegExpLiterals": true,
    "ignoreStrings": false,
    "ignoreTemplateLiterals": false
   }]
  }
}
